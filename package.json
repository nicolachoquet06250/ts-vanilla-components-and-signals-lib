{
  "name": "nc-signals-components",
  "private": false,
  "version": "3.11.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node src/server.js",
    "build": "tsc && vite build",
    "build:lib": "tsc -p tsconfig.build.json && node scripts/fix-extensions.js",
    "pack": "npm pack",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib",
    "publish:public": "npm run build:lib && npm publish --access public",
    "version:upgrade:patch": "npm version patch",
    "version:upgrade:minor": "npm version minor",
    "version:upgrade:major": "npm version major"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^24.10.1",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@types/express": "^5.0.5",
    "express": "^5.1.0"
  },
  "bin": {
    "ts2go": "scripts/ts2go.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/plugins/vite-plugin-autocomponent.js",
      "types": "./dist/plugins/vite-plugin-autocomponent.d.ts"
    },
    "./vite-plugin-autocomponent": {
      "import": "./dist/plugins/vite-plugin-autocomponent.js",
      "types": "./dist/plugins/vite-plugin-autocomponent.d.ts"
    }
  },
  "files": [
    "dist",
    "scripts/ts2go.js",
    "README.md",
    "DOCUMENTATION.md",
    "LICENSE"
  ],
  "sideEffects": false
}
